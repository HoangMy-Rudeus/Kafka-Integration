{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kafka": {
    "BootstrapServers": "localhost:9092",
    "GroupId": "order-service",
    "EnableAutoCommit": true,
    "AutoOffsetReset": "earliest",
    "SessionTimeoutMs": 6000,
    "MessageTimeoutMs": 30000
  }
}
